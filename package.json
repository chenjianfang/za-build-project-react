{
    "name": "@za-build/project-react",
    "version": "0.0.7",
    "description": "react构建脚手架",
    "scripts": {
        "dev": "webpack-dev-server --inline --color --host 0.0.0.0 --disable-host-check --config build/webpack.dev.conf.js",
        "prod": "cross-env NODE_ENV=production webpack --process --color --config build/webpack.prod.conf.js",
        "analyzer": "webpack --config ./build/webpack.prod.conf.js --profile --json > stats.json && webpack-bundle-analyzer stats.json -m static",
        "lint": "eslint --ext .js,.ts,tsx src",
        "lintfix": "eslint --fix --ext .js,.ts,tsx src"
    },
    "bin": {
        "za-dev-react": "./bin/dev.js",
        "za-prod-react": "./bin/prod.js",
        "za-eject-react": "./bin/eject.js",
        "za-build-react": "./bin/build.js"
    },
    "keywords": [
        "react",
        "build",
        "ZA"
    ],
    "author": "ZAFE",
    "license": "MIT",
    "files": [
        "bin",
        "build",
        "config",
        "shell",
        ".gitignore",
        "package.json",
        "readme.md"
    ],
